    "fsxa-ui": "0.1.38",
    "husky": "^4.2.5",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "semantic-release": "^17.1.1",
    "typescript": "~3.8.3",
    "vue-template-compiler": "^2.6.11",
