{
  "name": "fsxa-pattern-library",
  "version": "4.0.1",
  "author": {
    "name": "Tobias Golbs",
    "email": "golbs@e-spirit.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name fsxa-pattern-library src/index.ts",
    "build:local": "rimraf fsxa-pattern-library-*.tgz && npm version prerelease --no-git-tag-version --preid alpha && npm run build && npm pack",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "commit": "git-cz",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "prepush": "npm run build",
    "semantic-release": "semantic-release"
  },
  "main": "./dist/fsxa-pattern-library.umd.min.js",
  "files": [
    "dist",
    "package.json",
    "src/types",
    "src/constants.ts",
    "src/store",
    "README.md"
  ],
  "dependencies": {
    "fsxa-api": "^6.0.1",
    "prismjs": "^1.25.0",
    "setimmediate": "^1.0.5",
    "vue": "^2.6.14",
    "vue-prism-component": "^1.2.0",
    "vue-property-decorator": "8.2.2",
    "vue-tsx-support": "^2.3.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "autoprefixer": "10.4.16",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.8",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.4",
    "tailwindcss": "^3.3.6",
    "typescript": "^3.9.10",
    "vue-template-compiler": "^2.6.14"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "Apache-2.0",
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e-Spirit/fsxa-pattern-library.git"
  },
  "types": "src/types/fsxa-pattern-library.d.ts"
}
